generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model GuildConfig {
    defaultVolume     Int     @default(50)
    locale            String  @default("en-US")
    actionPermissions String?

    guildId String @id @unique

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("guild_config")
}
